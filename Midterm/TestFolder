package Midterm;

public class TestFolder {
	public static void main(String[] args) {
		Folder folder1 = new Folder("Class Material", 2, 2);
		File f1 = new File();
		folder1.addFile(f1);
		folder1.printFolderInfo();
		
		Folder folder2 = new Folder("Math", 0, 0);
		folder1.addFolder(folder2);
		folder1.printFolderInfo();
		
		File f2 = new File("aa");
		Folder folder3 = new Folder("Physic", 0, 0);
		folder1.addFile(f2);
		folder1.addFolder(folder3);
		folder1.printFolderInfo();
	}
}
